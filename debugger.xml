<application>
  <component name="XDebuggerSettings">
    <data-views />
    <general />
    <debuggers>
      <debugger id="oracle">
        <configuration>
          <option name="steppingPauseAtBegin" value="true" />
        </configuration>
      </debugger>
      <debugger id="python">
        <configuration>
          <option name="valuesPolicy" value="ON_DEMAND" />
          <option name="watchReturnValues" value="true" />
        </configuration>
      </debugger>
    </debuggers>
  </component>
</application>